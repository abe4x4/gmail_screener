{
  "date_range": {
    "after": "2024/01/01",
    "before": "2025/12/31"
  },
  "include": {
    "logical_operator": "OR",
    "terms": [
      "subject:receipt",
      "subject:invoice",
      "subject:order confirmation",
      "from:noreply@paypal.com",
      "from:amazon.com",
      "from:store.com",
      "purchase",
      "expense",
      "payment",
      "order #"
    ]
  },
  "exclude": {
    "logical_operator": "OR",
    "terms": [
      "subject:spam",
      "from:junk@example.com"
    ]
  }
}
